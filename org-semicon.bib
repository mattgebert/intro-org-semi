	file = {Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\SXLMQSWD\\Mogi et al. - 2017 - Tailoring tricolor structure of magnetic topologic.pdf:application/pdf;Mogi et al. - 2017 - Tailoring tricolor structure of magnetic topologic.pdf:C\:\\Users\\mgebert\\Zotero\\storage\\I4WFQDN8\\Mogi et al. - 2017 - Tailoring tricolor structure of magnetic topologic.pdf:application/pdf;Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\64RQFG7U\\eaao1669.html:text/html},
