@article{henke_x-ray_1993,
	title = {X-{Ray} {Interactions}: {Photoabsorption}, {Scattering}, {Transmission}, and {Reflection} at \textit{{E}} = 50-30,000 {eV}, \textit{{Z}} = 1-92},
	volume = {54},
	issn = {0092-640X},
