	keywords = {Quantum Hall, Topological insulators},
	pages = {390--394},
	file = {Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\PD6RQGAI\\Mogi et al. - 2022 - Experimental signature of the parity anomaly in a .pdf:application/pdf;Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\IWDZEHYZ\\s41567-021-01490-y.html:text/html},
