	journal = {Mater. Horiz.},
	author = {Marina, Sara and Gutierrez-Fernandez, Edgar and Gutierrez, Junkal and Gobbi, Marco and Ramos, Nicolás and Solano, Eduardo and Rech, Jeromy and You, Wei and Hueso, Luis and Tercjak, Agnieszka and Ade, Harald and Martin, Jaime},
	year = {2022},
	pages = {1196--1206},
	file = {d1mh01349a1.pdf:C\:\\Users\\mgebert\\Zotero\\storage\\KEPUHUE6\\d1mh01349a1.pdf:application/pdf;Marina et al. - 2022 - Semi-paracrystallinity in semi-conducting polymers.pdf:C\:\\Users\\mgebert\\Zotero\\storage\\JT96XMTI\\Marina et al. - 2022 - Semi-paracrystallinity in semi-conducting polymers.pdf:application/pdf},
