	file = {APS Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\SFNCQZHL\\PhysRevB.93.html:text/html;Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\VVD8RF6N\\Gan et al. - 2016 - Reexamination of the effective fine structure cons.pdf:application/pdf},
}

