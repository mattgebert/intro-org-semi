	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/aenm.202000757},
	keywords = {amorphous boron, Bi0.5Sb1.5Te3, composites, high ZT, thermoelectric materials},
	pages = {2000757},
	file = {Accepted Version:C\:\\Users\\mgebert\\Zotero\\storage\\HM4KXKF3\\Yang et al. - 2020 - Ultra-High Thermoelectric Performance in Bulk BiSb.pdf:application/pdf;Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\VTADIYT4\\aenm.html:text/html},
