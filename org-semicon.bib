	doi = {10.1103/PhysRevB.77.115449},
	abstract = {We theoretically calculate the phonon scattering limited electron mobility in extrinsic (i.e., gated or doped with a tunable and finite carrier density) two-dimensional graphene layers as a function of temperature (T) and carrier density (n). We find a temperature-dependent phonon-limited resistivity ρph(T) to be linear in temperature for T≳50K with the room-temperature intrinsic mobility reaching the values of above 105cm2/Vs. We comment on the low-temperature Bloch–Grüneisen behavior where ρph(T)∼T4 for unscreened electron-phonon coupling.},
	number = {11},
	urldate = {2018-06-01},
