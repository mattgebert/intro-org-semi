	note = {Publisher: American Physical Society},
	pages = {165440},
	file = {APS Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\PEGHPHE9\\PhysRevB.85.html:text/html;Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\GAGA2AJL\\Kaasbjerg et al. - 2012 - Unraveling the acoustic electron-phonon interactio.pdf:application/pdf},
}

@article{sugihara_thermoelectric_1983,
	title = {Thermoelectric power of graphite intercalation compounds},
	volume = {28},
	url = {https://link.aps.org/doi/10.1103/PhysRevB.28.2157},
	doi = {10.1103/PhysRevB.28.2157},
	abstract = {A model for the temperature variations of the thermoelectric power (TEP) of acceptor graphite intercalation compounds (GIC's) is presented. At low temperatures, the TEP in GIC's increases monotonically with T, then levels off above ∼200 K in striking contrast to that of pristine graphite. The diffusion contribution to the TEP is proportional to T and its magnitude is small as compared with that of the observed data. This observed behavior is attributed to the phonon drag effect. In the temperature region where the TEP is weakly temperature dependent, phonon relaxation is mainly controlled by the Rayleigh scattering due to point defects. The resultant TEP, which is composed of the phonon drag and diffusion terms, leads to a nearly T-independent value. Since the cross-sectional diameter of the Fermi surface in GIC's is larger than that of pristine graphite, the relaxation rate of the Rayleigh scattering, which is given by 1tI(q)=fq3, becomes very large at high temperatures (T{\textgreater}100 K). At low temperatures, where the boundary scattering plays a dominant role, the TEP is proportional to T3. Detailed calculations are carried out by solving the phonon-carrier-coupled Boltzmann equation.},
	number = {4},
	urldate = {2021-05-04},
	journal = {Phys. Rev. B},
	author = {Sugihara, Ko},
	month = aug,
	year = {1983},
	note = {Publisher: American Physical Society},
	pages = {2157--2165},
	file = {APS Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\5KNW28W2\\PhysRevB.28.html:text/html;Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\ISXDDJ63\\Sugihara - 1983 - Thermoelectric power of graphite intercalation com.pdf:application/pdf},
}

