@article{aradhya_nanosecond-timescale_2016,
	title = {Nanosecond-{Timescale} {Low} {Energy} {Switching} of {In}-{Plane} {Magnetic} {Tunnel} {Junctions} through {Dynamic} {Oersted}-{Field}-{Assisted} {Spin} {Hall} {Effect}},
