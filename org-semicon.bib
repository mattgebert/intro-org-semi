	file = {Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\IQVAKKSB\\Tang et al. - 2017 - Above 400-K robust perpendicular ferromagnetic pha.pdf:application/pdf;Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\272RL8WW\\e1700307.html:text/html},
