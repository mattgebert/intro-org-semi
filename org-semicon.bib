@article{idrus-saidi_liquid_2022,
	title = {Liquid metal synthesis solvents for metallic crystals},
	volume = {378},
	url = {https://www.science.org/doi/10.1126/science.abm2731},
	doi = {10.1126/science.abm2731},
	abstract = {In nature, snowflake ice crystals arrange themselves into diverse symmetrical six-sided structures. We show an analogy of this when zinc (Zn) dissolves and crystallizes in liquid gallium (Ga). The low-melting-temperature Ga is used as a “metallic solvent” to synthesize a range of flake-like Zn crystals. We extract these metallic crystals from the liquid metal solvent by reducing its surface tension using a combination of electrocapillary modulation and vacuum filtration. The liquid metal–grown crystals feature high morphological diversity and persistent symmetry. The concept is expanded to other single and binary metal solutes and Ga-based solvents, with the growth mechanisms elucidated through ab initio simulation of interfacial stability. This strategy offers general routes for creating highly crystalline, shape-controlled metallic or multimetallic fine structures from liquid metal solvents.},
	number = {6624},
	urldate = {2023-04-21},
	journal = {Science},
	author = {Idrus-Saidi, Shuhada A. and Tang, Jianbo and Lambie, Stephanie and Han, Jialuo and Mayyas, Mohannad and Ghasemian, Mohammad B. and Allioux, Francois-Marie and Cai, Shengxiang and Koshy, Pramod and Mostaghimi, Peyman and Steenbergen, Krista G. and Barnard, Amanda S. and Daeneke, Torben and Gaston, Nicola and Kalantar-Zadeh, Kourosh},
	month = dec,
	year = {2022},
	note = {Publisher: American Association for the Advancement of Science},
	pages = {1118--1124},
	file = {Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\KNWSA6LQ\\Idrus-Saidi et al. - 2022 - Liquid metal synthesis solvents for metallic cryst.pdf:application/pdf},
}

@article{wakafuji_3d_2020,
	title = {{3D} {Manipulation} of {2D} {Materials} {Using} {Microdome} {Polymer}},
	volume = {20},
	issn = {1530-6984},
	url = {https://doi.org/10.1021/acs.nanolett.9b05228},
	doi = {10.1021/acs.nanolett.9b05228},
	abstract = {We demonstrate 3D mechanical manipulations, such as sliding, rotating, folding, flipping, and exfoliating, of 2D materials using a microdome polymer (MDP) via in situ real-time observation with an optical microscope. A dimethylpolysiloxane (PDMS)-based MDP is covered with a poly(vinyl chloride) (PVC) adhesion layer. This PVC-MDP structure enables us to achieve small and adjustable contact areas between the PVC-MDP and a 2D-material flake, which is typically between ∼10 and ∼100 μm in diameter. The adhesion between the PVC polymer and 2D materials is fully tunable with temperature: Strong adhesion at ∼70 °C allows pick-up of the 2D material, and release occurs at ∼130 °C when the adhesion is weak. Thus the PVC-MDP functions as a point-of-contact manipulator for 2D materials, permitting the 3D manipulation of 2D-material flakes. Our method could facilitate the expansion of van der Waals heterostructure fabrication technology and the development of preparation techniques for more complex 3D structures.},
	number = {4},
	urldate = {2023-04-20},
	journal = {Nano Lett.},
	author = {Wakafuji, Yusai and Moriya, Rai and Masubuchi, Satoru and Watanabe, Kenji and Taniguchi, Takashi and Machida, Tomoki},
	month = apr,
	year = {2020},
	note = {Publisher: American Chemical Society},
	pages = {2486--2492},
	file = {ACS Full Text Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\CU6W2PHZ\\acs.nanolett.html:text/html;Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\DGCYKBGQ\\Wakafuji et al. - 2020 - 3D Manipulation of 2D Materials Using Microdome Po.pdf:application/pdf},
}

