	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Graphene, Electronic properties and materials, Semiconductors},
	pages = {4875},
	file = {Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\AMYGV4L7\\Avsar et al. - 2014 - Spin–orbit proximity effect in graphene.pdf:application/pdf},
