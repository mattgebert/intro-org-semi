	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Thermoelectrics, Thermoelectric devices and materials},
	pages = {9876},
	file = {Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\F83LX435\\Markov et al. - 2018 - Semi-metals as potential thermoelectric materials.pdf:application/pdf;Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\5CWCPT68\\s41598-018-28043-3.html:text/html},
}

@article{park_optimal_2021,
