@article{jang_tuning_2008,
	title = {Tuning the {Effective} {Fine} {Structure} {Constant} in {Graphene}: {Opposing} {Effects} of {Dielectric} {Screening} on {Short}- and {Long}-{Range} {Potential} {Scattering}},
