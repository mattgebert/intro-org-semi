	journal = {Phys. Rev. Lett.},
	author = {Mahapatra, Phanibhusan S. and Ghawri, Bhaskar and Garg, Manjari and Mandal, Shinjan and Watanabe, K. and Taniguchi, T. and Jain, Manish and Mukerjee, Subroto and Ghosh, Arindam},
