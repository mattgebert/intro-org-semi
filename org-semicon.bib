	note = {Number: 11
Publisher: Nature Publishing Group},
	keywords = {Two-dimensional materials, Electronic properties and materials},
	pages = {1029--1034},
	file = {41565_2019_547_MOESM1_ESM.pdf:C\:\\Users\\mgebert\\Zotero\\storage\\6SYBHGQR\\41565_2019_547_MOESM1_ESM.pdf:application/pdf;Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\CLAX4JB9\\Finney et al. - 2019 - Tunable crystal symmetry in graphene–boron nitride.pdf:application/pdf;Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\6XBZM2GL\\s41565-019-0547-2.html:text/html},
