	keywords = {axion insulators, quantum anomalous Hall effect, skyrmions, topological Hall effect, van der Waals heterostructures},
	pages = {2007795},
	file = {Advanced Materials - 2021 - Bhattacharyya - Recent Progress in Proximity Coupling of Magnetism to Topological Insulators.pdf:C\:\\Users\\mgebert\\Zotero\\storage\\I3QS8SYT\\Advanced Materials - 2021 - Bhattacharyya - Recent Progress in Proximity Coupling of Magnetism to Topological Insulators.pdf:application/pdf;Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\AL3U8FFY\\adma.html:text/html;Submitted Version:C\:\\Users\\mgebert\\Zotero\\storage\\QSAKUU3C\\Bhattacharyya et al. - 2021 - Recent Progress in Proximity Coupling of Magnetism.pdf:application/pdf},
