}

@article{du_approaching_2008,
	title = {Approaching ballistic transport in suspended graphene},
	volume = {3},
	copyright = {2008 Nature Publishing Group},
	issn = {1748-3395},
	url = {http://www.nature.com/articles/nnano.2008.199},
	doi = {10.1038/nnano.2008.199},
	abstract = {The discovery of graphene1,2 raises the prospect of a new class of nanoelectronic devices based on the extraordinary physical properties3,4,5,6 of this one-atom-thick layer of carbon. Unlike two-dimensional electron layers in semiconductors, where the charge carriers become immobile at low densities, the carrier mobility in graphene can remain high, even when their density vanishes at the Dirac point. However, when the graphene sample is supported on an insulating substrate, potential fluctuations induce charge puddles that obscure the Dirac point physics. Here we show that the fluctuations are significantly reduced in suspended graphene samples and we report low-temperature mobility approaching 200,000 cm2 V−1 s−1 for carrier densities below 5 × 109 cm−2. Such values cannot be attained in semiconductors or non-suspended graphene. Moreover, unlike graphene samples supported by a substrate, the conductivity of suspended graphene at the Dirac point is strongly dependent on temperature and approaches ballistic values at liquid helium temperatures. At higher temperatures, above 100 K, we observe the onset of thermally induced long-range scattering.},
	language = {en},
	number = {8},
	urldate = {2018-05-28},
	journal = {Nature Nanotechnology},
	author = {Du, Xu and Skachko, Ivan and Barker, Anthony and Andrei, Eva Y.},
	month = aug,
	year = {2008},
	pages = {491--495},
	file = {Du et al. - 2008 - Approaching ballistic transport in suspended graph.pdf:C\:\\Users\\mgebert\\Zotero\\storage\\M2RF7XIJ\\Du et al. - 2008 - Approaching ballistic transport in suspended graph.pdf:application/pdf;Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\REMA8S58\\nnano.2008.html:text/html},
}

@misc{cooper_experimental_2012,
	type = {Research article},
	title = {Experimental {Review} of {Graphene}},
	url = {https://www.hindawi.com/journals/isrn/2012/501686/},
	abstract = {This review examines the properties of graphene from an experimental perspective. The intent is to review the most important experimental results at a level of detail appropriate for new graduate students who are interested in a general overview of the fascinating properties of graphene. While some introductory theoretical concepts are provided, including a discussion of the electronic band structure and phonon dispersion, the main emphasis is on describing relevant experiments and important results as well as some of the novel applications of graphene. In particular, this review covers graphene synthesis and characterization, field-effect behavior, electronic transport properties, magnetotransport, integer and fractional quantum Hall effects, mechanical properties, transistors, optoelectronics, graphene-based sensors, and biosensors. This approach attempts to highlight both the means by which the current understanding of graphene has come about and some tools for future contributions.},
	language = {en},
	urldate = {2018-05-28},
	journal = {International Scholarly Research Notices},
	author = {Cooper, Daniel R. and D’Anjou, Benjamin and Ghattamaneni, Nageswara and Harack, Benjamin and Hilke, Michael and Horth, Alexandre and Majlis, Norberto and Massicotte, Mathieu and Vandsburger, Leron and Whiteway, Eric and Yu, Victor},
	year = {2012},
