	pages = {19970--19978},
	file = {ACS Full Text Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\YFTDTF22\\acs.jpcc.html:text/html;Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\TPDQJR6Z\\Frantti et al. - 2019 - In Quest of a Ferromagnetic Insulator Structure-C.pdf:application/pdf},
}

