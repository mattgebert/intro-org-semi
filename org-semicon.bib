	file = {ScienceDirect Full Text PDF:C\:\\Users\\mgebert\\Zotero\\storage\\2Y9DPITA\\Garmroudi et al. - 2022 - Large thermoelectric power factors by opening the .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\mgebert\\Zotero\\storage\\ZSHSHFCK\\S2542529322001407.html:text/html},
}

@article{hu_high_2021,
